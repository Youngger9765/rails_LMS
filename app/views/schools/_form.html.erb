<% if @school.errors.any? %>
    <ul>
        <% @school.errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
        <% end %>
    </ul>
<% end %>

<%= f.label :name, "Name" %>
<%= f.text_field :name %>

<%= f.label :description, "Description" %>
<%= f.text_area :description %>

<div class="field">
    <%= f.label :body %>
    <%= f.rich_text_area :body %>
</div>

